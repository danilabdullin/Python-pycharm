f_list = [7, 5, 3, 3, 2]
print(f_list)
number = int(input("Enter the number: "))
for el in range(len(f_list)):
    if f_list[el] == number:
        f_list.insert(el + 1, number)
        break
    elif f_list[el] > number and f_list[el + 1] < number:
        f_list.insert(el + 1, number)
        break
    elif f_list[-1] > number:
        f_list.append(number)
        break
    elif f_list[0] < number:
        f_list.insert(0, number)
        break
print(f_list)
